<category id="category_fu_serial" name="%{BKY_FU_SERIAL}" colour="%{BKY_FU_SERIAL_HUE}">
		<block type="fu_serial_begin_select">
			<field name="baudrate">115200</field>
		</block>	
		<block type="fu_serial_begin">
			<value name="baudrate">
				<shadow type="math_number">
					<field name="NUM">115200</field>
				</shadow>
			</value>
		</block>
		<block type="fu_serial_begin_config">
			<field name="config">SERIAL_8N1</field>
			<value name="baudrate">
				<shadow type="math_number">
					<field name="NUM">115200</field>
				</shadow>
			</value>
		</block>					
		<block type="fu_serial_print">
			<value name="data">
				<shadow type="text">
					<field name="TEXT"></field>
				</shadow>
			</value>
		</block>
		<block type="fu_serial_println">
			<value name="data">
				<shadow type="text">
					<field name="TEXT"></field>
				</shadow>
			</value>
		</block>
		<block type="fu_serial_newline">
		</block>			
		<block type="fu_serial_ready"></block>
		<block type="fu_serial_end"></block>

		<block type="fu_serial_print_format">
			<field name="format">BIN</field>
			<value name="data">
				<shadow type="math_number">
					<field name="NUM">0</field>
				</shadow>
			</value>
		</block>
		<block type="fu_serial_println_format">
			<field name="format">BIN</field>
			<value name="data">
				<shadow type="math_number">
					<field name="NUM">0</field>
				</shadow>
			</value>
		</block>
		<block type="fu_serial_write">
			<value name="data">
				<shadow type="math_number">
					<field name="NUM">0</field>
				</shadow>
			</value>
		</block>
		<block type="fu_serial_write_format">
			<field name="format">BIN</field>
			<value name="data">
				<shadow type="math_number">
					<field name="NUM">0</field>
				</shadow>
			</value>
		</block>		
</category>	